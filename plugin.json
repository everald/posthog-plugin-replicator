{
    "name": "Replicator + Static Filter",
    "url": "https://github.com/everald/posthog-plugin-replicator",
    "description": "Replicate PostHog event stream in another PostHog instance",
    "main": "index.ts",
    "config": [
        {
            "key": "host",
            "hint": "E.g. posthog.yourcompany.com",
            "name": "Host",
            "type": "string",
            "required": true
        },
        {
            "key": "project_api_key",
            "hint": "Grab it from e.g. https://posthog.yourcompany.com/project/settings",
            "name": "Project API Key",
            "type": "string",
            "required": true
        },
        {
            "key": "replication",
            "hint": "How many times should each event be sent",
            "name": "Replication",
            "type": "string",
            "default": "1",
            "required": false
        }
    ]
}
